		insertTr += ' <td><input type="checkbox" name="input_chk"/></td>';
		insertTr += ' <td><input type="checkbox" name="output_chk"/></td>';
		insertTr += ' <td><input type="checkbox" name="batch_chk"/></td>';
		insertTr += '</tr>';
		$("#tableColumnDataset").append(insertTr);
